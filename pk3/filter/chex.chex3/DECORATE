//Chex Quest 3
ACTOR FlemoidusCommonusV3Spawner : FlemoidusCommonusSpawner replaces FlemoidusCommonus
{
	States
	{
	Default:
		TNT1 A -1 A_SpawnItemEx("NazisFlemoidusCommonusV3",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION|SXF_TRANSFERSPECIAL|SXF_SETMASTER|SXF_CLEARCALLERTID,0,tid)
		Stop
	}
}
