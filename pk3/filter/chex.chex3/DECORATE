ACTOR NazisFlemoidusCommonusV3 : FlemoidusCommonusV3
{
	States
	{
	Death:
		TNT1 A 0 A_KillMaster
		Goto Super::Death
	XDeath:
		TNT1 A 0 A_KillMaster
		Goto Super::XDeath
	Ice:
		"####" "#" 5 A_GenericFreezeDeath
		"----" A 0 A_KillMaster
		"----" A 1 A_FreezeDeathChunks
		Wait
	}
}
